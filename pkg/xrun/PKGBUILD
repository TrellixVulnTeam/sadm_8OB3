pkgname=xrun
pkgver=1
pkgrel=1
pkgdesc="Run a command in the X context of the logged in user"
arch=('any')
license=('WTFPL')
depends=('bash')
source=("$pkgname")
sha256sums=('e2ebaa20f352c6288eeae991ed90c5b7833d599c5879e414b39cfebaeb1829bf')

package() {
	install $srcdir/$pkgname -D --target-directory $pkgdir/usr/bin/
}

