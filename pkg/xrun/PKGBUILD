pkgname=xrun
pkgver=2
pkgrel=1
pkgdesc="Run a command in the X context of the logged in user"
arch=('any')
license=('WTFPL')
depends=('bash')
source=("$pkgname")
sha256sums=('6ff7602f25ecc0b7273804faa591fb4091280f6db1285d754eb976da5035911f')

package() {
	install $srcdir/$pkgname -D --target-directory $pkgdir/usr/bin/
}

